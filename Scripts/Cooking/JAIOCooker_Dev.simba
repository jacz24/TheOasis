begin
  Login.AddPlayer(PNAME, PPASS, PPIN, []);
end;

begin
  Bot.Version              := 1;
  if (UPDATE) then
  begin
    //Self.UpdateOn        := UPDATE;
    //Self.LocalBotVersion := Bot.Version;
    //Self.BotType         := 'COOKERVERSION';
    //Self.Setup();
    //Script.ScriptUpdateHandler(); // Handles All Updating For CookerAIO
  end;
  if RSClient.IsLoggedIn() then
    Bot.init();
  Bot.Run();
end.
