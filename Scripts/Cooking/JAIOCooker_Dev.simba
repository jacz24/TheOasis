  PPASS        = '';    // Password
  PPIN         = '';    // PIN
(* Break settings *)
  TAKEBREAKS   = True; // Turn breaks on/off with True or False; Default: True
  BREAKAFTER   = 115; // Break after how long (in minutes); Default: 115
  BREAKFOR     = 24;  // Break for how long (in minutes); Default: 24

Type
