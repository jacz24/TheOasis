  i, attempts: Int32;
  ATPA: T2DPointArray;
  RangeFinder: TRSObjectFinder;
begin
  RangeFinder.ColorClusters += [CTS2(5330526, 23, 0.29, 0.25), CTS2(6777783, 9, 0.06, 1.54), 4]; // Couldron & Grill
  RangeFinder.ClusterDistance := 6;
  RangeFinder.Grow := 2;
  RangeFinder.Erode := 3;
  ATPA := MainScreen.FindObject(RangeFinder);
  if ATPA.Len < 1 then
  begin
