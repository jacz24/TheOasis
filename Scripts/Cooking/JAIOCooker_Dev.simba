  end;
  ATPA.SortByMiddle(Mainscreen.Center);

  for i:=0 to min(4, high(ATPA)) do
  begin
    Mouse.HumanMove(ATPA[i].Mean());
    if MainScreen.IsUpText(['Emerald','Benedict']) then Exit(True);
  end;
  if Bank.IsOpen() then Bank.Close();
end;

