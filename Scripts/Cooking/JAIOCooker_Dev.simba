  BankFinder.ColorClusters += [Self.BankCol, Self.WhiteBankCol, 7];
  //BankFinder.Grow := 2;
  BankFinder.ClusterDistance := 4;

  ATPA := MainScreen.FindObject(BankFinder);
  if ATPA.Len < 1 then
